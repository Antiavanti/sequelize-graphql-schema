{
  "name": "sequelize-graphql-schema",
  "version": "0.1.42",
  "description": "A helper library that lets you focus on business logic by automatically generates GraphQLSchema and manages graphQL from Sequelize model.",
  "main": "index.js",
  "scripts": {
    "build": "babel --plugins babel-plugin-transform-async-to-promises src --presets babel-preset-es2015 --out-dir dist",
    "check": "npm run lint && npm run test",
    "clean": "rm -f dist/*",
    "format": "prettier-eslint --write \"src/*.js\"",
    "jest": "jest",
    "jest-coverage": "npm run jest -- --coverage",
    "lint": "eslint src --cache",
    "test": "npm run test-db-reset && npm run jest",
    "test-db-reset": "rm -rf tests/database.db && sequelize db:migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alirizwan/sequelize-graphql-schema.git"
  },
  "keywords": [
    "graphql",
    "sequelize"
  ],
  "author": "Ali Rizwan <ali.aruz@outlook.com>",
  "contributors": [
    {
      "name": "Ali Rizwan",
      "email": "ali.aruz@outlook.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alirizwan/sequelize-graphql-schema/issues"
  },
  "homepage": "https://github.com/alirizwan/sequelize-graphql-schema#readme",
  "dependencies": {
    "camelcase": "^5.0.0",
    "dataloader-sequelize": "^1.7.3",
    "graphql-request": "^1.8.2",
    "graphql-sequelize": "^8.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-plugin-transform-async-to-promises": "^0.7.1",
    "eslint": "^3.17.1",
    "eslint-config-standard": "^7.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "jest-cli": "^19.0.2",
    "prettier-eslint-cli": "^3.1.2",
    "sequelize": "^4.37.10",
    "sequelize-cli": "^5.1.0",
    "sqlite3": "^4.0.2",
    "stringifier": "^1.3.0"
  },
  "peerDependencies": {
    "graphql": "^0.13.2",
    "sequelize": "^4.37.10"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true
  }
}
